{
  "name": "vidopc",
  "version": "0.0.1",
  "description": "Video files over OPC",
  "main": "lib/index.js",
  "scripts": {
    "build": "mkdir -p lib/proto && pbjs -t static-module -w es6 -o src/proto.js proto/*.proto && pbts -o src/proto.d.ts src/proto.js && babel src --out-dir lib --ignore src/*.d.ts -x .ts,.js",
    "prepare": "require-self",
    "prepare-example": "npm run-script build && lib/cli.js prepare examples/input.mp4 -o examples/output.vpc -c examples/vidopc.json",
    "preview-example": "npm run-script build && lib/cli.js preview examples/output.vpc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "vidopc": "lib/cli.js"
  },
  "author": "Kevin Montag <kmontag@cs.stanford.edu>",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "get-pixels": "^3.3.2",
    "opc": "^1.1.3",
    "progress": "^2.0.0",
    "promise-callbacks": "^3.1.0",
    "protobufjs": "~6.8.8",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/code-frame": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-typescript": "^7.0.0-rc.1",
    "@babel/register": "^7.0.0-rc.1",
    "@types/commander": "^2.12.2",
    "@types/fluent-ffmpeg": "^2.1.6",
    "@types/google-protobuf": "^3.2.7",
    "@types/node": "^10.7.1",
    "@types/progress": "^2.0.1",
    "require-self": "^0.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
